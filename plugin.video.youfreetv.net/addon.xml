<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.youfreetv.net"
       name="Youfreetv.net"
       version="1.0.2"
       provider-name="mrstealth">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
        <import addon="script.module.xbmc.helpers" version="2.0.1"/> 
    </requires>
    <extension point="xbmc.python.pluginsource" provides="video" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point= "xbmc.addon.metadata">
        <summary>Browse and stream live tv from youfreetv.net</summary>
        <description>Enables watching streams on youfreetv.net</description>
        <disclaimer>This is an unofficial XBMC addon</disclaimer>
        <platform>all</platform>
    </extension>
</addon>
