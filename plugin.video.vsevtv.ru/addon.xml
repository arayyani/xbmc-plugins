<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="plugin.video.vsevtv.ru"
       name="Vsevtv.ru"
       version="1.0.0"
       provider-name="mrstealth">
    <requires>
        <import addon="xbmc.python" version="2.0"/>
        <import addon="script.module.parsedom" version="0.9.1"/> 
    </requires>
    <extension point="xbmc.python.pluginsource" provides="video" library="default.py">
        <provides>video</provides>
    </extension>
    <extension point= "xbmc.addon.metadata">
        <summary>Stream live IPTV from vsevtv.ru</summary>
        <description>Enables watching streams on vsevtv.ru</description>
        <disclaimer>This is an unofficial XBMC add-on</disclaimer>
        <platform>all</platform>
    </extension>
</addon>
